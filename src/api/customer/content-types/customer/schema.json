{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "CustomerID": {
      "type": "uid"
    },
    "Name": {
      "type": "string"
    },
    "PhoneNumber": {
      "type": "integer"
    },
    "CredentialID": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::credential.credential",
      "inversedBy": "CustomerID"
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "customer"
    }
  }
}
